function removeDuplicates(nums) {
    let i = 0;
    for (let j = 1; j < nums.length; j++) {
        if (nums[j] !== nums[i]) {
            i++;
            nums[i] = nums[j];
        }
    }
    return i + 1;
}
let nums = [1, 1, 2];
console.log(removeDuplicates(nums)); // Output: 2 (nums becomes [1, 2, _])
